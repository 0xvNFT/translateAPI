<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryNew</item>
        <item name="colorPrimaryDark">@color/colorPrimaryNew</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:fontFamily">@font/inter</item>
    </style>

    <style name="Nav.NavigationDrawer" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorControlHighlight">@color/colorPrimaryNew</item>
    </style>

    <style name="AppTheme.AdAttribution">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">12sp</item>
        <item name="android:text">AD</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:background">@drawable/grnativeadattribute</item>
        <item name="android:width">22dp</item>
        <item name="android:height">22dp</item>
    </style>

    <style name="AppTheme.AdAttributionNew">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">12sp</item>
        <item name="android:text">AD</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:background">@drawable/grnativeadattributenew</item>
        <item name="android:width">22dp</item>
        <item name="android:height">22dp</item>
    </style>


</resources>
